<Window x:Class="DSS_WPF.ResultsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="1000" ContentRendered="Window_ContentRendered">
    <StackPanel Orientation="Horizontal">
        <lvc:CartesianChart Height="400" UpdaterTick="CartesianChart_UpdaterTick" DisableAnimations="True" Hoverable="False" DataTooltip="{x:Null}" Series="{Binding SeriesCollection1}" VerticalAlignment="Top" HorizontalAlignment="Left" Width="500" />
        <lvc:CartesianChart Height="400" UpdaterTick="CartesianChart_UpdaterTick" DisableAnimations="True" Hoverable="False" DataTooltip="{x:Null}" Series="{Binding SeriesCollection2}" VerticalAlignment="Top" HorizontalAlignment="Right" Width="500" />
    </StackPanel>
</Window>
