                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="600" />
                    <RowDefinition Height="600" />
                </Grid.RowDefinitions>
                <StackPanel HorizontalAlignment="Left" Margin="0,0,0,0" Grid.Row="0" Grid.Column="0">

                    <TextBlock Height="auto" Margin="10,35,0,0"><Underline><Run FontWeight="Bold" Text="Proefstuk 3"/></Underline></TextBlock>
                    <TextBlock Height="auto" Margin="10,30,0,0"><Run FontWeight="Bold" Text="Voor beproeving"/></TextBlock>
                    <TextBlock Text="Boring:" Height="auto" Margin="10,10,0,0"/>
                    <TextBlock Text="Monster:" Height="auto" Margin="10,10,0,0"/>
                    <TextBlock Text="Bus:" Height="auto" Margin="10,10,0,0"/>
                    <TextBlock Text="Diepte maaiveld t.o.v. NAP (m):" TextWrapping="Wrap" Height="auto" Margin="10,10,0,0"/>
                    <TextBlock Text="Monsterdiepte t.o.v. maaiveld (m):" Height="auto" Margin="10,10,0,0"/>
                    <TextBlock Text="Monsterdiepte t.o.v. NAP (m):" Height="auto" Margin="10,10,0,0"/>
                    <TextBlock Text="Monsterklasse:" Height="auto" Margin="10,10,0,0"/>
