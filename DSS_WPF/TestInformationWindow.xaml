                <StackPanel Margin="0,72,0,598" Grid.Row="0" Grid.Column="1" Grid.RowSpan="2">

                    <TextBox Text=""  Height="auto" Margin="10,35,0,0"/>
                    <TextBox Text=""  Height="auto" Margin="10,8,0,0"/>
                    <TextBox Text=""  Height="auto" Margin="10,8,0,0"/>
                    <TextBox Text=""  Height="auto" Margin="10,8,0,0"/>
                    <TextBox Text=""  Height="auto" Margin="10,8,0,0"/>
                    <TextBox Text=""  Height="auto" Margin="10,8,0,0"/>
                    <TextBox Text=""  Height="auto" Margin="10,8,0,0"/>
                    <TextBox Text=""  Height="auto" Margin="10,8,0,0"/>

                    <TextBox Text=""  Height="auto" Margin="10,54,0,0"/>
                    <TextBox Text=""  Height="auto" Margin="10,8,0,0"/>
                    <TextBox Text=""  Height="auto" Margin="10,8,0,0"/>


                    <TextBox Text=""  Height="auto" Margin="10,54,0,0"/>
